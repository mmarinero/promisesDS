{
        "extends": "eslint:recommended",
        "ecmaFeatures": {"jsx": true},
        "env": {
            "browser": true,
            "node": true,
            "es6": true
        },
        "rules": {
            "no-extra-semi": 0,
            "no-unused-vars": [2, {"args": "all", "argsIgnorePattern": "^_"}],
            "no-console": 0,
            "semi": [2, "always"],
        }
    }
